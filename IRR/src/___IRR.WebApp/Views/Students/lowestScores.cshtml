
@model List<Homework31.WebApp.Models.Student>
@{
    
    ViewBag.Title = "News Page";
}

<div class="jumbotron">
    <h1>M101.NET Blog</h1>
    <p class="lead">News From MongoDB</p>
    <p><a href="http://education.mongodb.com" class="btn btn-primary btn-lg">Learn more &raquo;</a></p>
</div>

<div class="row">
    <table border="1">

        @foreach (Homework31.WebApp.Models.Student student in Model)
        {
            <tr>
                <td>@student.Id</td>
                <td>@student.name</td>
                <td>
                    <table border="1">
                        @foreach (Homework31.WebApp.Models.Score score in student.scores)
                        {
                            <tr>

                                <td>@score.type</td>
                                <td>@score.score</td>
                            </tr>
                        }
                    </table>
                </td>
</tr>
        }
        </table>
    </div>





